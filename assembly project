.model small
.stack 10000d
.386
.data
    oneChar db ?, '$'
    numbers dw 20000 Dup(?)
    minusChar dd ?
    lengthArr dw ?
    innerLooCn dw ?
    outerLoopCnt dw ?
    
    
.code
start:
mov ax, @data
mov ds, ax
call readNumbers
call println

mov, ax 4C00h
int 21h

 readNumbers PROC
        xor di, di
        xor si, si
        mov numbers[0], 0
        mov minusChar, '+'
read next:
   mov ah, 2Fh
            mov bx, 0h 
            mov cx, 1  
            mov dx, offset oneChar  
            int 21h   

            or ax, ax
            je stop


 println PROC
        mov ah, 02h
        mov dl, 10
        int 21h

        xor ax, ax
        xor dx, dx

        ret
    println ENDP
